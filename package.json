{
  "name": "webpack-basic",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js --progress --port 8082",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "dll": "webpack --config config/webpack.dll.js",
    "lib": "webpack --config config/webpack.lib.js",
    "check-type": "tsc",
    "commit": "git cz",
    "lint-staged": "lint-staged",
    "lint": "eslint ./src/**/*.{js,jsx,vue,ts,tsx} --fix",
    "commitlint": "commitlint --config commitlint.config.js -e -V",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "yarn lint",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@nuxt/friendly-errors-webpack-plugin": "^2.5.2",
    "@nuxtjs/friendly-errors-webpack-plugin": "^2.1.0",
    "@types/lodash-es": "^4.17.5",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vue/compiler-sfc": "^3.2.21",
    "add-asset-html-webpack-plugin": "^3.2.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.4",
    "copy-webpack-plugin": "^9.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.1.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "eslint-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.0.2",
    "mini-css-extract-plugin": "^2.4.4",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "style-loader": "^3.3.1",
    "typescript": "^4.4.4",
    "vue-loader": "^16.8.3",
    "webpack": "^5.62.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "core-js": "^3.19.1",
    "jQuery": "^1.7.4",
    "lodash-es": "^4.17.21",
    "vue": "^3.2.21",
    "vue-router": "^4.0.12"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
